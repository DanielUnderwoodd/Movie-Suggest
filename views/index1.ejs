<% extend('layout') %>
<br>
<ul class="list-group">
  <div class="float-left">
    <% movies.forEach(function(movie){ %>
    <br>
    <br>
    <h1 class="display-4"> <%= movie.title  %></h1>
       <a href="/movie/<%= movie._id %>" >   <img src="/css/<%= movie.image%>.jpg" class="img-fluid" alt="Responsive image">  </a>  
    <% }); %>
  </div>
  </ul>

